<?xml version="1.0" encoding="windows-1252"?>
<!DOCTYPE faces-config PUBLIC
  "-//Sun Microsystems, Inc.//DTD JavaServer Faces Config 1.1//EN"
  "http://java.sun.com/dtd/web-facesconfig_1_1.dtd">
<faces-config xmlns="http://java.sun.com/JSF/Configuration">
  <navigation-rule>
    <from-view-id>/Departments.jspx</from-view-id>
    <navigation-case>
      <from-outcome>GotoAnotherPage</from-outcome>
      <to-view-id>/AnotherPage.jspx</to-view-id>
    </navigation-case>
  </navigation-rule>
  <lifecycle>
    <phase-listener>oracle.adf.controller.faces.lifecycle.ADFPhaseListener</phase-listener>
  </lifecycle>
  <application>
    <default-render-kit-id>oracle.adf.core</default-render-kit-id>
  </application>
  <navigation-rule>
    <from-view-id>/AnotherPage.jspx</from-view-id>
    <navigation-case>
      <from-outcome>BackToDepartments</from-outcome>
      <to-view-id>/Departments.jspx</to-view-id>
    </navigation-case>
  </navigation-rule>
  <navigation-rule>
    <from-view-id>/YetAnotherPage.jspx</from-view-id>
    <navigation-case>
      <from-outcome>BackToDepartments</from-outcome>
      <to-view-id>/DepartmentsProgrammatic.jspx</to-view-id>
    </navigation-case>
  </navigation-rule>
  <navigation-rule>
    <from-view-id>/DepartmentsProgrammatic.jspx</from-view-id>
    <navigation-case>
      <from-outcome>GotoAnotherPage</from-outcome>
      <to-view-id>/YetAnotherPage.jspx</to-view-id>
    </navigation-case>
  </navigation-rule>
  <managed-bean>
    <managed-bean-name>DepartmentsProgrammatic</managed-bean-name>
    <managed-bean-class>test.view.backing.DepartmentsProgrammatic</managed-bean-class>
    <managed-bean-scope>request</managed-bean-scope>
  </managed-bean>
  <navigation-rule>
    <from-view-id>/DepartmentsAlwaysRequeryOnPageEntry.jspx</from-view-id>
    <navigation-case>
      <from-outcome>GotoAnotherPage</from-outcome>
      <to-view-id>/YetAnotherPage2.jspx</to-view-id>
    </navigation-case>
  </navigation-rule>
  <navigation-rule>
    <from-view-id>/YetAnotherPage2.jspx</from-view-id>
    <navigation-case>
      <from-outcome>BackToDepartments</from-outcome>
      <to-view-id>/DepartmentsAlwaysRequeryOnPageEntry.jspx</to-view-id>
    </navigation-case>
  </navigation-rule>
  <managed-bean>
    <managed-bean-name>DepartmentsTimedAutoRefresh</managed-bean-name>
    <managed-bean-class>test.view.backing.DepartmentsTimedAutoRefresh</managed-bean-class>
    <managed-bean-scope>request</managed-bean-scope>
  </managed-bean>
  <managed-bean>
    <managed-bean-name>UserInfo</managed-bean-name>
    <managed-bean-class>test.view.managed.UserInfo</managed-bean-class>
    <managed-bean-scope>session</managed-bean-scope>
  </managed-bean>
</faces-config>