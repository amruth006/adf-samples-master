      if (currentCountryId != null) {
        CountriesImpl country = (CountriesImpl)CountriesImpl.getDefinitionObject()
                                                             .findByPrimaryKey(getDBTransaction(), 
                                                                               CountriesImpl.createPrimaryKey(currentCountryId));
