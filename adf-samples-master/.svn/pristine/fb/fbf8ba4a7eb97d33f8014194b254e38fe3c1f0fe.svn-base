    public void createNewEmployee(String Ename, String Job,Number Mgr,Date Hiredate, Number Sal, Number Comm, Number Deptno) {
        ViewObject empVO = getEmployees();
        EmpViewRowImpl e = (EmpViewRowImpl)empVO.createRow();
        empVO.insertRow(e);
        e.setEname(Ename);
        e.setJob(Job);
        e.setMgr(Mgr);
        e.setHiredate(Hiredate);
        e.setComm(Comm);
        e.setSal(Sal);
        e.setDeptno(Deptno);
        try  {
            getDBTransaction().commit();
        } catch (JboException ex)  {
            getDBTransaction().rollback();
            throw ex;
        } 
    }

