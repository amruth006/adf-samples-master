<?xml version="1.0" encoding="windows-1252"?>
<!DOCTYPE faces-config PUBLIC
  "-//Sun Microsystems, Inc.//DTD JavaServer Faces Config 1.1//EN"
  "http://java.sun.com/dtd/web-facesconfig_1_1.dtd">
<faces-config xmlns="http://java.sun.com/JSF/Configuration">
  <lifecycle>
    <phase-listener>test.view.fwkext.CustomADFPhaseListener</phase-listener>
  </lifecycle>
  <application>
    <default-render-kit-id>oracle.adf.core</default-render-kit-id>
  </application>
  <navigation-rule>
    <from-view-id>/ListPage.jspx</from-view-id>
    <navigation-case>
      <from-outcome>Edit</from-outcome>
      <to-view-id>/EditPage.jspx</to-view-id>
    </navigation-case>
    <navigation-case>
      <from-outcome>Add</from-outcome>
      <to-view-id>/AddPage.jspx</to-view-id>
    </navigation-case>
    <navigation-case>
      <from-outcome>TryFindMerge</from-outcome>
      <to-view-id>/FindOrMergePage.jspx</to-view-id>
    </navigation-case>
  </navigation-rule>
  <navigation-rule>
    <from-view-id>/EditPage.jspx</from-view-id>
    <navigation-case>
      <from-outcome>BackToList</from-outcome>
      <to-view-id>/ListPage.jspx</to-view-id>
    </navigation-case>
  </navigation-rule>
  <managed-bean>
    <managed-bean-name>UserInfo</managed-bean-name>
    <managed-bean-class>test.view.managed.UserInfo</managed-bean-class>
    <managed-bean-scope>session</managed-bean-scope>
  </managed-bean>
  <managed-bean>
    <managed-bean-name>ListPage</managed-bean-name>
    <managed-bean-class>test.view.backing.ListPage</managed-bean-class>
    <managed-bean-scope>request</managed-bean-scope>
    <managed-property>
      <property-name>bindings</property-name>
      <value>#{bindings}</value>
    </managed-property>
  </managed-bean>
  <navigation-rule>
    <from-view-id>/AddPage.jspx</from-view-id>
    <navigation-case>
      <from-outcome>BackToList</from-outcome>
      <to-view-id>/ListPage.jspx</to-view-id>
    </navigation-case>
  </navigation-rule>
  <managed-bean>
    <managed-bean-name>AddPage</managed-bean-name>
    <managed-bean-class>test.view.backing.AddPage</managed-bean-class>
    <managed-bean-scope>request</managed-bean-scope>
    <managed-property>
      <property-name>bindings</property-name>
      <value>#{bindings}</value>
    </managed-property>
  </managed-bean>
  <managed-bean>
    <managed-bean-name>EditPage</managed-bean-name>
    <managed-bean-class>test.view.backing.EditPage</managed-bean-class>
    <managed-bean-scope>request</managed-bean-scope>
    <managed-property>
      <property-name>bindings</property-name>
      <value>#{bindings}</value>
    </managed-property>
  </managed-bean>
  <managed-bean>
    <managed-bean-name>AppInfo</managed-bean-name>
    <managed-bean-class>test.view.managed.AppInfo</managed-bean-class>
    <managed-bean-scope>application</managed-bean-scope>
    <managed-property>
      <property-name>expectedApplicationExceptionPackageNames</property-name>
      <list-entries>
        <value>test.model.exceptions</value>
      </list-entries>
    </managed-property>
  </managed-bean>
  <managed-bean>
    <managed-bean-name>FindOrMergePage</managed-bean-name>
    <managed-bean-class>test.view.backing.FindOrMergePage</managed-bean-class>
    <managed-bean-scope>request</managed-bean-scope>
  </managed-bean>
  <navigation-rule>
    <from-view-id>/FindOrMergePage.jspx</from-view-id>
    <navigation-case>
      <from-outcome>BackToList</from-outcome>
      <to-view-id>/ListPage.jspx</to-view-id>
    </navigation-case>
  </navigation-rule>
</faces-config>