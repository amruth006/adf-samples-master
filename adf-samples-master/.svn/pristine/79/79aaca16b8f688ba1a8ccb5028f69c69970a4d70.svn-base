    <table id="DeptView" IterBinding="DeptViewIterator">
      <AttrNames>
        <Item Value="Deptno"/>
        <Item Value="Dname"/>
        <Item Value="Loc"/>
      </AttrNames>
    </table>
  </bindings>
</pageDefinition>
